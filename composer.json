{
    "name": "owinteractive/manageable",
    "authors": [
        {
            "name": "Fausto Rodrigues",
            "email": "fausto.rodrigues@owinteractive.com"
        }
    ],
    "require": {
        "laravel/framework": "5.4.*|5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.2|~6.0",
        "orchestra/testbench": "~3.5",
        "squizlabs/php_codesniffer": "^2.3",
        "orchestra/database": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Ow\\Manageable\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ow\\Manageable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ow\\Manageable\\ServiceProvider"
            ],
            "aliases": {
                "Manageable": "Ow\\Manageable\\Facade"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
